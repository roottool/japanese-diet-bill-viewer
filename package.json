  "description": "This viewer visualizes various data on the Japanese Diet bills.",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/japanese-diet-bill-viewer.git"
  },
  "workspaces": [
    "web",
    "workers"
  ],
  "prisma": {
